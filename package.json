{
  "name": "LUNZ-UI",
  "version": "2.0.0",
  "private": true,
  "description": "这个项目是使用 [ngX-Rocket](https://github.com/ngx-rocket/generator-ngx-rocket/)\r version 1.3.1 生成的。",
  "main": "karma.conf.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env && ng build --aot --prod",
    "build:stage": "npm run env && ng build --aot --dev --output-hashing=all --environment=stage",
    "start": "ng serve --open",
    "lint": "ng lint && stylelint \"src/app/**/*.scss\" --syntax scss && htmlhint \"src/app\" --config .htmlhintrc",
    "lint:all": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "translations:extract": "ng2-translate-extract --dir ./src --output ./src/translations --format=json --clean",
    "docs": "hads ./docs -o",
    "env": "env2json npm_package_version -o src/environments/.env.json",
    "generate": "ng generate",
    "stage": "npm run lint && git checkout stage && git merge develop --no-edit --no-ff && npm run build:stage && git add . && git commit -m \"release stage\" && git push && git checkout develop",
    "live": "npm run lint && git checkout live && git merge master --no-edit --no-ff && npm run build && git add . && git commit -m \"release live\" && git push && git checkout master",
    "clean": "del-cli dist"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ngx-translate/core": "^6.0.0",
    "@swimlane/ngx-datatable": "10.0.5",
    "@toverux/ngx-sweetalert2": "3.0.0",
    "@zhongruigroup/ngx-query": "^1.0.1",
    "zr-progress": "^1.1.0",
    "angular-tree-component": "7.0.2-beta1",
    "angular2-moment": "^1.9.0",
    "animate.css": "^3.5.2",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-datepicker": "1.7.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "highcharts": "^5.0.15",
    "html-webpack-plugin": "^3.0.6",
    "intl": "^1.2.5",
    "jquery": "^3.2.1",
    "lint": "^1.1.2",
    "lodash": "^4.17.4",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "ng-select": "^1.0.0-beta.6",
    "ng2-signalr": "^2.1.1",
    "ng2-tooltip-directive": "^2.0.2",
    "ng2-trim-directive": "^2.1.4",
    "ng2-validation": "^4.2.0",
    "ngx-bootstrap": "^2.0.5",
    "ngx-pipes": "^2.1.7",
    "ngx-select-dropdown": "^0.2.1",
    "ngx-toastr": "^6.5.0",
    "oidc-client": "^1.5.0-beta.2",
    "popper.js": "^1.12.6",
    "rxjs": "^5.4.3",
    "signalr": "^2.2.2",
    "sweetalert2": "^7.0.5",
    "urijs": "^1.18.10",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.9",
    "@angular/compiler-cli": "^4.4.6",
    "@biesbjerg/ng2-translate-extract": "^0.6.0",
    "@types/jasmine": "2.5.45",
    "@types/lodash": "^4.14.52",
    "@types/node": "^7.0.0",
    "codelyzer": "~2.0.0",
    "del-cli": "^1.1.0",
    "env2json": "^1.0.0",
    "hads": "^1.3.1",
    "htmlhint": "^0.9.13",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "~1.5.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~5.1.0",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "~3.0.2",
    "tslint": "~4.5.1",
    "typescript": "2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhongruiGroup/LUNZ-UI.git"
  },
  "keywords": [],
